@using Entities.Concrete
@model List<Category>
@{
    Layout = null;
}

<!DOCTYPE html>

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Categori adi </th>
        <th>Başlık</th>
        <th>Categori durumu</th>
        <th>Categori tanımı</th>
    </tr>
    <tr>
        @foreach (var item in Model)
        {
            <td>@item.Id</td>
            <td>@item.CategoryName</td>
           // <td>@item.Headings</td>
            <td>@item.CategoryStatus</td>
            <td>@item.CategoryDescription</td>
        }
      
    </tr>
</table>

